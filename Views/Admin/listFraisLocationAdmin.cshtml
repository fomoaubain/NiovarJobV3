@model IEnumerable<WebApplication1.Models.FraisLocation>
@{
    ViewBag.Title = "listFraisLocationAdmin";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Liste des transactions monétaires sur location employés</h2>

<!-- Search Filter -->

<div class="table-responsive mt-4">
    <table class="datatable table table-stripped mb-0">
        <thead>
            <tr>
                <th>
                    Client
                </th>
                <th>
                    Payé le
                </th>
                <th>
                   Montant a payé
                </th>
                <th>
                  Avance
                </th>
                <th>
                  Reste
                </th>
            </tr>
        </thead>

        @foreach (WebApplication1.Models.FraisLocation item in ViewBag.listFraisLocation)
        {
            <tr>
                <td>
                    @item.getPayeur().nom [@item.getPayeur().email]
                </td>
                <td>
               @item.created
                </td>
                <td>
                    @item.montant $
                </td>
                <td>
                    @item.avance $
                </td>

                <td>
                    @item.reste $
                </td>
                
    
            </tr>
        }

    </table>

</div>
